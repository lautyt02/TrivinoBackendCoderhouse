<h1>Chat en Tiempo Real</h1>
<p id="bienvenida"></p>
<div>
    <input type="text" id="chatBox">
</div>
<div id="messageLogs">
    {{#each messages}}
    <h4>{{this.user}}</h2>
    <p>{{this.message}}</p>
    <hr>
    {{/each}}
</div>
<script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
<script src="/socket.io/socket.io.js"></script>
<script src="/js/chat.js"></script>